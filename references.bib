@article{Ferrera2016EffCom,
  title = {Effective Competition Determines the Global Stability of Model Ecosystems},
  author = {Ferrera, Antonio and Pascual-García, Alberto and Bastolla, Ugo},
  date = {2016-11-28},
  journaltitle = {Theoretical Ecology},
