@article{Song2017WhyArea,
  title = {Why Are Some Plant–Pollinator Networks More Nested than Others?},
  author = {Song, Chuliang and Rohr, Rudolf P. and Saavedra, Serguei},
  date = {2017},
  journaltitle = {Journal of Animal Ecology},
  volume = {86},
  pages = {1417--1424},
  issn = {1365-2656},
  doi = {10.1111/1365-2656.12749},
  url = {https://besjournals.onlinelibrary.wiley.com/doi/abs/10.1111/1365-2656.12749},
  urldate = {2021-03-03},
  abstract = {Empirical studies have found that the mutualistic interactions forming the structure of plant–pollinator networks are typically more nested than expected by chance alone. Additionally, theoretical studies have shown a positive association between the nested structure of mutualistic networks and community persistence. Yet, it has been shown that some plant–pollinator networks may be more nested than others, raising the interesting question of which factors are responsible for such enhanced nested structure. It has been argued that ordered network structures may increase the persistence of ecological communities under less predictable environments. This suggests that nested structures of plant–pollinator networks could be more advantageous under highly seasonal environments. While several studies have investigated the link between nestedness and various environmental variables, unfortunately, there has been no unified answer to validate these predictions. Here, we move from the problem of describing network structures to the problem of comparing network structures. We develop comparative statistics, and apply them to investigate the association between the nested structure of 59 plant–pollinator networks and the temperature seasonality present in their locations. We demonstrate that higher levels of nestedness are associated with a higher temperature seasonality. We show that the previous lack of agreement came from an extended practice of using standardized measures of nestedness that cannot be compared across different networks. Importantly, our observations complement theory showing that more nested network structures can increase the range of environmental conditions compatible with species coexistence in mutualistic systems, also known as structural stability. This increase in nestedness should be more advantageous and occur more often in locations subject to random environmental perturbations, which could be driven by highly changing or seasonal environments. This synthesis of theory and observations could prove relevant for a better understanding of the ecological processes driving the assembly and persistence of ecological communities.},
  annotation = {\_eprint: https://besjournals.onlinelibrary.wiley.com/doi/pdf/10.1111/1365-2656.12749},
  file = {/Users/tanyastrydom/Zotero/storage/77K3WZ66/Song et al. - 2017 - Why are some plant–pollinator networks more nested.pdf;/Users/tanyastrydom/Zotero/storage/ZBQ5GSE4/1365-2656.html},
  keywords = {changing environments,nestedness,network comparison,seasonality,structural stability,z-scores},
  langid = {english},
  number = {6}
}

@article{Staniczenko2017LinMac,
  title = {Linking Macroecology and Community Ecology: Refining Predictions of Species Distributions Using Biotic Interaction Networks},
  shorttitle = {Linking Macroecology and Community Ecology},
  author = {Staniczenko, Phillip P. A. and Sivasubramaniam, Prabu and Suttle, K. Blake and Pearson, Richard G.},
