  eprint = {2007.04448},
  eprinttype = {arxiv},
  file = {/Users/tanyastrydom/Zotero/storage/CZGAKTMQ/Kawakatsu et al. - 2021 - Emergence of Hierarchy in Networked Endorsement Dy.pdf},
  keywords = {Computer Science - Social and Information Networks,Nonlinear Sciences - Adaptation and Self-Organizing Systems,Physics - Physics and Society},
  langid = {english},
  primaryclass = {nlin, physics:physics}
}

@article{Kefi2019AdvOur,
